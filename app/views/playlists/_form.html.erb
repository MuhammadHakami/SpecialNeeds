<%= form_for @playlist do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <br>
  <%= f.label :description %>
  <%= f.text_field :description %>
  <br>
  <%= f.label :rating %>
  <%= f.number_field :rating %>
  <br>
  <%= f.label :level %>
  <%= f.text_field :level %>
  <br>
  <%= f.hidden_field :user_id, :value => current_user.id %>
  <%= f.label :image %>
  <%= f.url_field :image %>
  <br>
<%= f.submit "Submit" %>
<% end %>


